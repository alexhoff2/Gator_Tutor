{
  "name": "gator-tutor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build && cp -r .next/static .next/standalone/.next/static",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "setup": "npm ci && npx prisma generate && npm run build",
    "clean": "rm -rf .next node_modules",
    "db:seed": "ts-node --esm --project ./prisma/tsconfig.seed.json ./prisma/seed.ts",
    "db:seed:fresh": "ts-node --esm --project prisma/tsconfig.seed.json prisma/seed.ts --fresh",
    "build:clean": "rm -rf .next && npm run build",
    "build:low-memory": "NODE_OPTIONS='--max_old_space_size=512 --gc-interval=100' npm run build",
    "build:part1": "NODE_OPTIONS='--max_old_space_size=512' next build --no-lint",
    "build:part2": "cp -r .next/static .next/standalone/.next/static || true",
    "build:micro": "npm run build:part1 && npm run build:part2",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "prisma migrate deploy",
    "wait-for-db": "node -e \"const net=require('net');const client=net.createConnection({host:'db',port:3306},()=>process.exit(0));client.on('error',()=>setTimeout(()=>{console.log('Waiting for DB...');process.exit(1)},1000))\"",
    "docker:init": "npm run wait-for-db && npm run prisma:generate && npm run prisma:deploy",
    "db:init": "docker compose exec db mysql -u root -pmysql -e \"CREATE USER IF NOT EXISTS 'gator_user'@'%' IDENTIFIED BY 'gator_pass'; GRANT ALL PRIVILEGES ON *.* TO 'gator_user'@'%' WITH GRANT OPTION; FLUSH PRIVILEGES;\"",
    "db:seed:docker": "NODE_OPTIONS='--loader ts-node/esm' node --require ts-node/register/transpile-only --project ./prisma/tsconfig.seed.json ./prisma/seed.ts",
    "db:setup": "npm run db:init && docker compose exec web npx prisma migrate dev --name init && docker compose exec web node --loader ts-node/esm prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.4",
    "@prisma/client": "5.21.1",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-context-menu": "^2.2.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-hover-card": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.3",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "date-fns": "^4.1.0",
    "decimal.js": "10.4.3",
    "jose": "^5.9.6",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "0.454.0",
    "next": "14.2.16",
    "react": "18.2.0",
    "react-day-picker": "^9.3.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.53.1",
    "reactflow": "^11.11.4",
    "sharp": "0.33.1",
    "sonner": "^1.7.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@shadcn/ui": "^0.0.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/eslint": "^8.56.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "20.11.19",
